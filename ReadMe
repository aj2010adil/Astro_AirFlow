# We can run below in docker play linux machine which is free to use
curl -sSL https://install.astronomer.io | sudo bash
mkdir astro 
cd astro 
astro dev init
astro dev start
## now login to 8080 page and provide password
sudo apt install python3-pip
sudo pip install apache-airflow
AIRFLOW_HOME=/opt/airflow
sudo airflow db init
